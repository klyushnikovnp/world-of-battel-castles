<header>
	{{ page.top_line }}
	{{ page.branding }}
	{{ page.header }}
</header>

  {{ page.highlighted }}

  {{ page.help }}
  
<main role="main" class="flex-grow-1">
	<div class="layout-container container">
		<div class="row">
			{% if page.left_sidebar %}
			  <div class="left-sidebar col-lg-4">
				{{ page.left_sidebar }}
			  </div>
			{% endif %}
			<div class="content col-12 col-lg-{{ (page.left_sidebar)? '8':'12' }}">
			  {{ page.breadcrumb }}
			  {{ page.content }}
			</div>
		</div>
	</div>
</main>

<footer class="mt-5">
	<div class="footerWrapper">
		<div class="container h-100">
			<div class="footerInner d-flex flex-column justify-content-end h-100">
				<div class="py-3 footer text-white p14">
					<div class="row align-items-end">
						{{ page.footer }}
					</div>
				</div>
			</div>
		</div>
	</div>
</footer> 






